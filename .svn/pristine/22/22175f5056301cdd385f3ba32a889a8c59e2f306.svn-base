@charset "UTF-8";
body {
  font-family: "Arial";
  background-color: #edf2ee;
}

body,
ul,
li,
ol,
dl,
dd,
dt,
p {
  padding: 0;
  margin: 0;
}

ul,
li,
ol {
  list-style: none;
}

input,
button {
  -webkit-appearance: none;
  border-radius: 0;
  border: none;
}

a {
  text-decoration: none;
  color: inherit;
}

textarea {
  -webkit-appearance: none;
}
