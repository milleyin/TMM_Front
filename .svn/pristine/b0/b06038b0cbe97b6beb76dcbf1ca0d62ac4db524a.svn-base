<!-- 点的详情页面 -->
<div class="navbar tmm-navbar">
  <div class="navbar-inner">
    <div class="left">
      <a href="#" class="back link">
        <i class="icon ticon-arrow-back"></i>
      </a>
    </div>
    <div class="center sliding">{{itemObj.name}}</div>
    <div class="right">
      <!-- <a href="#">
        <i class="icon ticon-praise"></i>
      </a> -->
    </div>
  </div>
</div>
<div class="pages">
  <!-- Page, "data-page" contains page name -->
  <div data-page="item-detail" class="page tmm-item-detail">
    <div class="page-content">
      <div class="banner">
        <ul>
          <li><img class="lazy" data-src="{{itemObj.img}}"></li>
        </ul>
      </div>
      <div class="info">
        <div class="info-list"><span class="tit">开放时间</span><span class="txt">{{itemObj.start_work}} - {{itemObj.end_work}}</span></div>
        <div class="info-list"><span class="tit">联系电话</span><span class="txt">{{itemObj.phone}}</span></div>
        <div class="info-list"><span class="tit">微信号</span><span class="txt">{{itemObj.weixin}}</span></div>
        <div class="info-list address"><span class="tit">地址</span><span class="txt">{{itemObj.address}}</span><a href="javascript:;" class="showmore" id="showmoreMap" data-address="{{itemObj.address}}" data-city="{{itemObj.address_arr.city}}"><i class="icon ticon-map"></i></a></div>
        <div class="info-list item-con">
          <div class="tit">内容</div>
          <div class="txt">
          {{#each itemObj.fare}}
            <div class="item-list"><div class="item-name">{{name}} </div><div class="item-info">{{info}} </div><div class="item-price">{{price}} 元</div></div>
          {{/each}}
          </div>
        </div>
      </div>
      <!-- 内容添加 -->
      <div class="edit_content">
        {{itemObj.content}}
      </div>
    </div>
    <div class="tmm-more-map" id="tmm-hotel-map" style="display:none">
      <div class="tmm-more-title">{{itemObj.address}}</div>
      <img src="{{itemObj.map}}">
    </div>
    <div class="tmm-bg-mask" id="tmm-hotel-mask" style="display:none"></div>
  </div>
</div>
