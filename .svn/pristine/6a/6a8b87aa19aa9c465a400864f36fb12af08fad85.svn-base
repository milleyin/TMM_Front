<div class="participant-item" ng-repeat="item in model.list_data">
  <div class="time">{{item.add_time}}</div>
  <div class="con">
    <div class="title">姓名：{{item.name}}</div>
    <div class="title">联系电话：{{item.phone}}</div>
    <div class="title">参与人数：{{item.people}}成人，{{item.children}}儿童</div>
  </div>
</div>



