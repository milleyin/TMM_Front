.tmm-item-field{width: 100%;}
.tmm-item-field .item-field{-webkit-box-sizing: border-box;white-space: nowrap;display: inline-block;text-overflow: ellipsis;}
.tmm-item-field .item-field:nth-child(1) {width: 30%;}
.tmm-item-field .item-field:nth-child(2) {width: 25%;}
