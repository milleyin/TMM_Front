<!-- 我发起的活动 -->
<ion-view view-title="银行卡管理" cache-view="false" hide-tabs="true">
  <ion-nav-buttons>
    <i class="tmm-ion-left tmm-withdraw-left ion-android-arrow-back" ui-sref="tab.mywallet" nav-direction="back"></i>
  </ion-nav-buttons>
  
  <ion-content class="tmm-my-bankcard-content tmm-my-wallet">
    <div class="tmm-card-top">
      <img src="images/card1.png"/>
    </div>

  	<ion-list class="tmm-withdraw-list tmm-bankcard-list">
      <ion-item>
        <div class="tmm-bankcard-title">真实姓名</div>
        <div class="tmm-bankcard-content">{{json.bank_name}}</div>
      </ion-item>
      <ion-item>
        <div class="tmm-bankcard-title">开户银行</div>
        <div class="tmm-bankcard-content">{{json.bank_value}}{{json.bank_branch}}</div>
      </ion-item>
      <ion-item>
        <div class="tmm-bankcard-title">银行卡号</div>
        <div class="tmm-bankcard-content">{{json.bank_code_format}}</div>
      </ion-item>
    </ion-list>

    <div class="tmm-message-button">
      <button class="tmm-message-sub tmm-bankcard-btnbg" type="submit" ui-sref="tab.changebank">更换银行卡</button>
    </div>

  </ion-content>
</ion-view>


