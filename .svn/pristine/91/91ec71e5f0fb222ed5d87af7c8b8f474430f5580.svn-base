*{
	margin: 0;
	padding: 0;
}
.title,.title a,.title a:hover,.title a:visited{
  color: #403a39;
  font-size: 16px;
}
.title a:hover,.title a:visited{
  font-weight: bold;
}
a:link,a:visited,a:active,a:hover {
	text-decoration: none;/*清除超链接下划线*/
}
.cantent p{
	font-size: 20px;
}
/*清除浮动*/
.clear{
	clear:both;
}
.content_box{
	font-size: 9px;
  width: 96%;
  margin: 0 auto;
  margin-top: 72px;
  position: relative; /* 弹出层 */
  z-index: 100;
}
.box_div {
    position: relative;
    z-index: 100;
    width: 100%;
    margin-top: 30px;
}
/*已选商品的虚线框*/
.box {
	margin-top: 0px;
    width: 90%;
    border: 1px dashed #d0860e;
    padding-top: 15px;
    padding-bottom: 15px;
    position: relative;
	background: #fcf1df;
}
.box p{
	font-size: 11px;
	margin: 40px;
	margin-left: 90px;
}
.box span{
	color: blue !important;
}
.box span a{
	color: blue !important;
}
.box1{
	margin-top: 0px;
    width: 100%;
    border: 1px dashed #d0860e;
    padding-top: 15px;
    padding-bottom: 15px;
    position: relative;
	background: #FBEED5;
	border-radius:10px;
}
.box2{
	margin-top: 0px;
    width: 100%;
    border: 1px dashed #d0860e;
    padding-top: 15px;
    padding-bottom: 15px;
    position: relative;
	background: #FBEED5;
	border-radius:10px;
}
.box1 p{
	font-size: 20px;
	padding-top: 30px;
	padding-bottom: 10px;
	color: #159300 !important;
}
.box1 span{
	color: black;
}
.box2 span{
	font-size: 16px;
	margin:0 30px;/*span与span之间的距离*/
}
.box2 span a{
	font-size: 13px;
	color: blue;
}
.a{
	text-align: right;
	margin-right: 13px;
}
.box1 a{
	color: blue;
}
.state_normal {
    color: #159300 !important;
}
/*矩形框*/
.box_div .title2{
  position: absolute; /*绝对定位*/
  top:0px;  
  left:30px; 
  z-index: 103;
  width: 80px;
  height: 20px;
  font-size: 9px;
  color: #403a39;
  border: 1px dashed #d0860e;
  background: #fcf1df;
  text-align: center;
  padding-top: 6px;/*调整里面的字*/
}
.cantent{
	border: 1px dashed #d0860e;
	margin: 90px 0px 0px 110px;
	padding: 50px;
	width: 50%;
	background: bisque;
	border-radius:10px;
}
.cantent_zhu{
	margin: 40px 0px 0px 80px;
	padding: 10px;
	width: 50%;
}
.cantent table{
	padding: 20px;
	margin-left: 20px;
	font-size: 9px;
	text-align: left;
	line-height:30px;/*字体之间的行距*/
}
.de td:first-child{
	text-align: right;
}
.call{
	font-size: 11px;
	background: #655a58;
	color: #fff;
	text-align: center;
	height: 30px;
	width: 88px;
	margin-right: 10px;
	margin-top: 30px;
	border: black;
}
.cantent_zhu_call{
	font-size: 11px;
	background: #655a58;
	color: #fff;
	text-align: center;
	height: 30px;
	width: 88px;
	margin-top: 80px;
	border: black;
}
.cantent_top1{
	float: left;
	width: 45%;
	margin-right: 20px;
}
.cantent_top2{
	float: left;
	width: 45%;
}
.cantent_top3{
	width: 94%;
}
.ke{
	margin: 10px;
	margin-top: 25px;
	margin-bottom: 35px;
}
.cantent_ment{
	margin: 100px;
}
.ment_left{
	float: left;
}
.ment_right{
	float: left;
	margin: 20px 0px 0px 40px;
	font-size: 20px;
}
.ment_right a{
	font-size: 16px;
	color: blue;	
}
.ment_right a:first-child{
	font-size: 20px;
}




