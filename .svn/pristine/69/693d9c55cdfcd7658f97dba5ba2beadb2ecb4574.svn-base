<div class="navbar tmm-navbar">
  <div class="navbar-inner">
    <div class="left">
      <a href="#" class="back link">
        <i class="icon ticon-arrow-back"></i>
      </a>
    </div>
    <div class="center">活动报名详情</div>
    <div class="right">
    </div>
  </div>
</div>
<div class="pages">
  <div data-page="order-detail" class="page page-order tmm-my-order-detail-page" id="tmm-order-line-detail">
    <div class="page-content tmm-order-detail">

      <div class="content-block-title tmm-content-title">出游日期</div>
      <div class="list-block tmm-retinue tmm-main-retinue tmm-retinue-top-border tmm-retinue-bottom-border">
        <div class="row tmm-order-status">
          <div>{{orderObj.go_time}}</div>
        </div>
      </div>

      <!--项目列表-->
      <div class="content-block-title tmm-content-title">项目列表</div>

      <div class="list-block tmm-list-block tmm-retinue-top-border tmm-retinue-bottom-border">
    {{#each orderObj.items_fare}}
        <div class="tmm-day-num">{{showDay @key}}</div>
        {{#each this}}
          {{#each this}}
        <ul class="tmm-order-ul-before">
          <li class="tmm-item-li clearfix b-border">
            <div class="tmm-item-title">
              {{name}}
             
          

            </div>
            <div class="tmm-order-item-content">
            {{#each fare}}
              {{#js_compare "this.number != '0' "}}
              <div class="tmm-item-fare">
                <div class="tmm-fare-first w_40">{{name}}</div>

                {{#js_compare "this.room_number == '0' "}}
                <span class="w_20">{{info}}</span>
                {{/js_compare}}
                {{#js_compare "this.room_number != '0' "}}
                <span class="w_20">{{room room_number}}</span>
                {{/js_compare}}
                <span class="w_10">{{number}}</span>
                <span class="w_20">￥{{count}}</span>
              </div>
              {{/js_compare}}
            {{/each}}
            </div>
          </li>
        </ul>
          {{/each}}
        {{/each}}
    {{/each}}   
      </div>

      {{#js_compare "this.is_organizer.value =='1'"}}
      <div class="content-block-title tmm-content-title">
        <div class="tmm-order-safe-pay">服务费（交通、导服及其他）</div>
      </div>
      <div class="list-block tmm-retinue tmm-main-retinue tmm-retinue-top-border tmm-retinue-bottom-border">
        <div class="row tmm-order-status tmm-order-fare-item">
          <span class="fare-item w_40">服务费￥{{orderObj.user_price_fact.value}}/人</span>
          <span class="fare-item w_40">{{orderObj.user_go_count.value}}份</span>
        </div>
      </div>
      {{/js_compare}}

      <!--项目列表-->
      <div class="content-block-title tmm-content-title">主要联系人</div>
      <div class="list-block tmm-retinue tmm-main-retinue tmm-retinue-top-border tmm-retinue-bottom-border">
        <div class="row tmm-order-status">
      {{#each orderObj.retinue}}
        {{#js_compare "this.is_main == 1 "}}
          <div>{{name}}</div>
        {{/js_compare}}
      {{/each}}  
        </div>
      </div>
      {{#js_compare "this.hasRetinue == 1 "}}
        <div class="content-block-title tmm-content-title">随行人员<!-- （{{js " this.orderObj.user_go_count.value - 1  "}}人） --></div>
        <div class="list-block tmm-retinue tmm-main-retinue tmm-retinue-top-border tmm-retinue-bottom-border">
          <div class="row  tmm-order-status">

        {{#each orderObj.retinue}}
          {{#js_compare "this.is_main == 0 "}}
            <div class="w_33">{{name}}</div>
          {{/js_compare}}
        {{/each}}

          </div>
        </div>
      {{/js_compare}}

      <div class="content-block-title tmm-content-title">合计价格</div>
      <div class="list-block tmm-retinue tmm-main-retinue tmm-retinue-top-border tmm-retinue-bottom-border">
        <div class="row tmm-order-status">
          <div>订单总价<span class="tmm-total-price">￥{{orderObj.order_price.value}}</span></div>
        </div>
      </div>
      <div class="content-block-title tmm-content-title">订单状态</div>
      <div class="list-block tmm-retinue tmm-main-retinue tmm-retinue-top-border tmm-retinue-bottom-border">
        <div class="row tmm-order-status">
          <div>订单编号：{{orderObj.order_no}}</div>
        </div>
      </div>
      
    </div>
  </div>
</div>
