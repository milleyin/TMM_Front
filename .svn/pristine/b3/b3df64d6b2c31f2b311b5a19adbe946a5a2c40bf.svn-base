<div class="page active" ng-style="active.frame">
  <div class="navbar">
    <div></div>
    <div class="title">{{active.model.name}}</div>
    <div></div>
  </div>
  <div class="content">
    <div class="scroll-content">
      <div class="banner"><img ng-src="{{active.model.list_img}}"></div>
      <div class="intro">
        <div class="box col-6">
          <div class="box-tit">参与人数</div>
          <div class="box-txt">{{active.model.actives_info.order_number.sign}} / {{active.model.actives_info.number.value}}</div>
        </div>
        <div class="box col-6">
          <div class="box-tit">觅趣状态</div>
          <div class="box-txt">{{active.model.actives_status.name}}</div>
        </div>
        <div class="box col-6">
          <div class="box-tit">报名时间</div>
          <div class="box-txt">{{active.model.actives_time.start_time}} / {{active.model.actives_time.end_time}}</div>
        </div>
        <div class="box col-6">
          <div class="box-tit">出游日期</div>
          <div class="box-txt">{{active.model.actives_time.go_time}}</div>
        </div>
        <div class="box col-12">
          <div class="box-tit">觅趣详情</div>
          <div class="active-intro" ng-bind-html="active.active_intro"></div>
        </div>
      </div>

      <div ng-repeat="attr in active.model.list" class="shop-wrap">
        <div class="schedule">
          <i class="bracket">&lceil;</i> {{attr.day_content}}
          <i class="bracket">&rfloor;</i>
        </div>
        <div class="schedule-day">
          <div class="circle"></div>
          <div class="day">{{attr.day}}</div>
          <div class="arrow"></div>
        </div>
        <div class="items" ng-repeat="attr2 in attr.dot_list">
          <div class="tmm-img-pos" ng-repeat-start="attr3 in attr2.day_item"></div>
          <div class="item-title">{{attr3.item_name}}</div>
          <div class="item-address">
            {{attr3.address}}
          </div>
        
          <a ng-repeat-end class="img">
            <img ng-src="{{attr3.item_img}}" class="img">
          </a>
        </div>
      </div>
    </div>
  </div>
  <div class="toolbar">
    <a class="center" style="display: block;width: 100%;text-align: center;" ng-href="#/apply/{{active.id}}">我也要报名</a>
  </div>
</div>
