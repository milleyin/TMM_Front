}

.show-grid {
    margin-bottom: 15px;
    border-bottom: 2px dotted #c1c1bf;
    margin-right: 0;
    margin-left: 0;
}

.show-grid.grid-no-border {
    border-bottom: none;
}

.container.gdu-container {
    /*width: 1240px;*/
    /*padding-right: 0;
      padding-left: 0;
      margin-right: auto;
      margin-left: auto;*/
}

.navbar.gdu-navbar {
    padding-left: 60px;
    background-color: #fff;
}

.container.gdu-navbar-container {
    padding: 30px 0;
    margin-right: auto;
    margin-left: auto;
}


/* 小于或等于 767px时 */

@media (max-width: 767px) {
    body {
        padding-top: 60px;
    }
    .container > .navbar-header,
    .container-fluid > .navbar-header,
    .container > .navbar-collapse,
    .container-fluid > .navbar-collapse {
        margin-right: 0;
        /*margin-left: -60px;*/
        border-color: #eaeaea;
    }
    .navbar.gdu-navbar {
        /*background-color: red;*/
        height: 51px;
    }
    .container.gdu-navbar-container {
        padding: 0 0 0 15px;
        margin-left: -60px;
        background-color: #fff;
    }
    .gdu-list-group {
        display: none;
    }
    .col-xs-article-img {
        float: left;
        width: 136px;
        /*margin-right: 2px;*/
    }
    .col-xs-article-title {
        float: left;
        width: 50%;
        /*margin-left: 2px;*/
        padding: 0;
        padding-left: 12px;
    }
}

@media (min-width: 768px) and (max-width: 991px) {}

@media (min-width: 992px) and (max-width: 1199px) {}

@media (min-width: 1200px) {}

.navbar.gdu-navbar .navbar-toggle {
    background: #fff;
    border: none;
}

.navbar.gdu-navbar .navbar-toggle span {
    background-color: #333;
}

.navbar-collapse.gdu-navbar-collapse {
    padding-top: 5px;
}

.navbar-brand.gdu-navbar-brand {
    margin: 0 15px;
    padding: 0;
    font-size: 24px;
    color: #000;
}

.navbar-brand.gdu-navbar-brand:hover,
.navbar-brand.gdu-navbar-brand:focus {
    color: #000;
}


/* logo */

.gdu-logo {
    margin-top: 5px;
    width: 80px;
    height: 40px;
    line-height: 40px;
    border: none;
}

.navbar-inverse.gdu-navbar-inverse {
    border-bottom: 1px solid #eaeaea;
    box-shadow: 0px 2px 0px 0px #fafafa;
}

.gdu-navbar-inverse.navbar-inverse .navbar-nav > li {
    margin: 0 10px;
    font-size: 16px;
    color: #333;
}

.gdu-navbar-inverse.navbar-inverse .navbar-nav > li > a,
.gdu-navbar-inverse.navbar-inverse .navbar-nav > .active > a {
    display: block;
    padding: 10px 20px;
    cursor: pointer;
    color: #333;
}

.gdu-navbar-inverse.navbar-inverse .navbar-nav > li > a:hover,
.gdu-navbar-inverse.navbar-inverse .navbar-nav > li > a:focus {
    background: #ededed;
    border-radius: 16px;
}

.gdu-navbar-inverse.navbar-inverse .navbar-nav > .active > a,
.gdu-navbar-inverse.navbar-inverse .navbar-nav > .active > a:hover,
.gdu-navbar-inverse.navbar-inverse .navbar-nav > .active > a:focus {
    background: #ededed;
    border-radius: 16px;
}


/* 关注我们 */

.gdu-follow-me {
    margin-top: 0;
    font-size: 24px;
}

.list-group.gdu-list-group {
    padding-left: 15px;
}


/* 幻灯片  */

.jumbotron.gdu-jumbotron {
    padding: 0 0;
}

.gdu-swiper-container {
    height: 310px;
}

@media (max-width: 767px) {
    .jumbotron.gdu-jumbotron {
        margin-left: -15px;
        margin-right: -30px;
    }
    .gdu-swiper-container {
        height: 150px;
    }
}

.gdu-swiper-container img {
    width: 100%;
    height: 100%;
}

.gdu-swiper-container .swiper-pagination-bullet {
    width: 12px;
    height: 12px;
    opacity: 1;
    background-color: #fff;
}

.gdu-swiper-container .swiper-pagination-bullet-active {
    background-color: #ff843c;
}


/* 前面左边文章部分 */

.gdu-row-left {
    margin: 0;
}

.gdu-row-left .gdu-article-cover img {
    width: 100%;
    height: 150px;
}

.gdu-row-left .gdu-title,
.gdu-row-left .gdu-title a {
    padding: 0;
    margin: 0;
    font-size: 24px;
    font-weight: 800;
    color: #333;
    text-decoration: none;
}

.gdu-row-left .gdu-title a:hover {
    color: #5a6e91;
    text-decoration: none;
}

.gdu-row-left .gdu-options {
    padding: 10px 0;
}

.gdu-row-left .gdu-options .gdu-time {
    margin-right: 10px;
}

.gdu-row-left .gdu-options .gdu-time,
.gdu-row-left .gdu-options .gdu-type {
    color: #5a6e91;
    font-size: 15px;
}

.gdu-row-left .gdu-info {
    color: #888;
    height: 100px;
    overflow: hidden;
    word-wrap: break-word;
    word-break: break-all;
}

@media (max-width: 767px) {
    .show-grid {
        padding-bottom: 10px;
    }
    .gdu-row-left .gdu-article-cover img {
        width: 136px;
        height: 75px;
    }
    .gdu-row-left .gdu-title a {
        font-size: 13px;
        color: #333;
        word-wrap: break-word;
    }
    .gdu-row-left .gdu-options .gdu-time {
        margin-right: 5px;
    }
    .gdu-row-left .gdu-options .gdu-time,
    .gdu-row-left .gdu-options .gdu-type {
        font-size: 11px;
        color: #8d8d8d;
    }
    .gdu-row-left .gdu-info {
        display: none;
    }
}


/*更多文章*/

.gdu-article-row {
    margin: 0;
}

.gdu-article-more {
    margin: 10px 0;
    padding: 10px 0;
    text-align: center;
    color: #888;
    background-color: #f5f5f5;
    cursor: pointer;
}


/* 专题 */


/* 左边部分 */

.gdu-row-left-special {
    margin: 0;
}

.gdu-row-left-special .gdu-special-cover img {
    width: 100%;
    height: 290px;
    margin-bottom: 10px;
}

.gdu-row-left-special .gdu-special-title,
.gdu-row-left-special .gdu-special-title a {
    padding: 0;
    margin: 0;
    font-size: 24px;
    font-weight: 800;
    color: #333;
    text-decoration: none;
}

.gdu-row-left-special .gdu-special-title a:hover {
    color: #5a6e91;
    text-decoration: none;
}

.gdu-row-left-special .gdu-special-options {
    padding: 10px 0;
}

.gdu-row-left-special .gdu-special-options .gdu-article-num {
    color: #5a6e91;
    font-size: 15px;
}

.gdu-row-left-special .gdu-special-info {
    color: #888;
    height: 200px;
    overflow: hidden;
    word-wrap: break-word;
    word-break: break-all;
}

@media (max-width: 767px) {
    .col-xs-special-img {
        float: left;
        width: 162px;
    }
    .col-xs-special-title {
        padding: 0;
        padding-left: 12px;
    }
    .gdu-row-left-special .gdu-special-cover img {
        width: 162px;
        height: 96px;
        /*scale();*/
    }
    .gdu-row-left-special .gdu-special-title a {
        font-size: 14px;
    }
    .gdu-row-left-special .gdu-special-options .gdu-article-num {
        font-size: 12px;
        color: #8d8d8d;
    }
    .gdu-row-left-special .gdu-special-info {
        display: none;
    }
}


/* 专题详情 */

.gdu-special-category {
    position: relative;
}

.gdu-special-category-img {
    width: 100%;
    height: 325px;
}

.gdu-category-detail {
    position: absolute;
    top: 35px;
    right: 35px;
    padding-left: 10px;
    padding-right: 10px;
    width: 300px;
    background-color: #fff;
    opacity: 0.7;
}

@media (max-width: 767px) {
    .gdu-special-category {
        margin-left: -15px;
        margin-right: -30px;
    }
    .gdu-special-category-img {
        /*width: 577px;*/
        height: 162px;
    }
    .gdu-category-detail {
        top: 12px;
        right: 10px;
        width: 40%;
        height: 80px;
    }
    .gdu-category-detail h3 {
        margin-top: 8px;
        font-size: 14px;
    }
    .gdu-category-detail h5 {
        font-size: 12px;
    }
    .gdu-category-detail p {
        display: none;
    }
}


/* 全部文章 */

.article-lists {
    margin: 0;
    padding: 0;
    width: 100%;
}

.article-lists li {
    list-style: disc inside;
    color: #e9e9e9;
    font-size: 16px;
    margin-bottom: 20px;
}

.article-lists li a {
    width: 50%;
    display: inline-block;
    color: #333333;
    text-decoration: none;
    margin-right: 20px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.article-lists li span {
    font-size: 12px;
    color: #dadada;
}

.article-lists li:hover,
.article-lists li:hover a {
    color: #5a6e91;
}

.search-row {
    margin-bottom: 15px;
    margin-right: 0;
    margin-left: 0;
    color: #888;
}


/* 普通文章详情 */

.gdu-article-normal-row {
    padding-right: 0;
}

.gdu-article-options {
    margin: 20px 0;
    color: #888888;
}

.gdu-article-time {
    margin-right: 10px;
}

.gdu-article-description {
    border: 1px solid #e6e6e6;
    padding: 10px 0px;
    position: relative;
}

.gdu-article-description-remark {
    position: absolute;
    display: block;
    width: 14px;
    height: 40px;
    top: 50%;
    left: 10px;
    margin-top: -20px;
    font-size: 14px;
    color: #666666;
    border-left: 1px solid #e6e6e6;
    border-right: 1px solid #e6e6e6;
    padding-left: 10px;
    padding-right: 20px;
}

.gdu-article-description-content {
    min-height: 20px;
    font-size: 14px;
    color: #888888;
    padding-left: 50px;
    padding-right: 5px;
    line-height: 24px;
}

.gdu-article-content img {
    width: 100%;
    height: 100%;
}

@media(max-width: 767px) {
    .gdu-article-title {
        font-size: 26px;
    }
}


/* 相册文章详情2 */

.gdu-article-albumn-info-wrap {
    position: relative;
}

.gdu-cover-plan-info {
    position: absolute;
    top: 20px;
    left: 0;
    padding-left: 30px;
    width: 180px;
    text-align: right;
    opacity: 0.7;
}

.gdu-cover-plan-info .gdu-cover-plan {
    width: 150px;
    height: 100px;
}

.gdu-cover-plan-info .gdu-cover-img-num {
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    color: #fff;
    text-align: center;
    background-color: #e90010;
}

.gdu-article-albumn-swiper-container .swiper-button-prev,
.gdu-article-albumn-swiper-container .swiper-button-next {
    /*top: 30%;*/
}

.gdu-article-albumn-swiper-container {
    height: 100%;
}

.gdu-article-albumn-swiper-container img {
    width: 100%;
    max-height: 100%;
    height: auto;
}

.gdu-article-albumn-info {
    padding-left: 10px;
    padding-right: 10px;
}

.gdu-article-albumn-title {
    margin-top: 10px;
    font-size: 24px;
}

.gdu-article-albumn-options {
    color: #666;
    font-size: 14px;
    margin-bottom: 10px;
}

.gdu-article-albumn-date {
    margin-right: 10px;
}

.gdu-article-albumn-description {
    border-top: 2px solid #666;
    padding-top: 10px;
    font-size: 14px;
    color: #666;
}


/* 其它屏 */

.gdu-article-albumn-other-info {
    position: relative;
    padding-top: 10px;
    padding-left: 10px;
}

.gdu-page-num-info-other {
    position: absolute;
    width: 80px;
    height: 107px;
    padding: 10px 10px;
    overflow: hidden;
    background: url(../images/line.png) 0 0 no-repeat;
}

.gdu-page-num-info-other .gdu-curNum-other {
    position: absolute;
    width: 45px;
    text-align: right;
    left: 10px;
    top: 3px;
    color: #444444;
    font: 32px/1.5 "Microsoft YaHei", "Comic Sans MS", "Verdana";
}

.gdu-page-num-info-other .gdu-allNum-other {
    position: absolute;
    left: 58px;
    top: 55px;
    color: #cfcfcf;
    font-weight: bold;
}

.gdu-article-albumn-other-description {
    min-height: 107px;
    padding-left: 90px;
    padding-right: 10px;
}


/* 相册文章详情 */

.gdu-special-albumn-body {
    padding-top: 115px;
}

.container-fluid.gdu-container-fluid {
    padding-right: 0;
    padding-left: 0;
}

.gdu-row-fluid .col-xs-12,
.gdu-row-fluid .col-md-12 {
    padding-right: 0;
    padding-left: 0;
}

.gdu-swiper-container-special {}

.gdu-swiper-container-special img {
    width: 100%;
    height: 100%;
    border: none;
}


/* 幻灯片的第一页图 */

.swiper-slide.gdu-special-slide-cover {
    position: relative;
    color: #fff;
}

.gdu-special-slide-cover-img {
    position: absolute;
    top: 100px;
    left: 200px;
    width: 450px;
    height: 280px;
    border: none;
    opacity: 0.8;
    text-align: center;
    background-color: #ddd;
}

.gdu-special-img-nums {
    display: inline-block;
    padding: 30px 10px;
    background-color: #e90010;
}

.gdu-special-slide-content {
    position: absolute;
    top: 120px;
    left: 720px;
    right: 200px;
}

.gdu-special-slide-title {
    border-bottom: 7px solid #d8d8d8;
    color: #e90010;
    cursor: pointer;
    font: bold 48px/1.5 "Microsoft YaHei", "Comic Sans MS", "Verdana";
}

.gdu-special-slide-options {
    color: #888;
    font-size: 16px;
}

.gdu-special-slide-time {
    margin-right: 10px;
}


/*  其它图 */

.gdu-page-num-info {
    position: relative;
    height: 107px;
    padding: 10px 10px;
    overflow: hidden;
    border-bottom: 1px solid #dbdbdb;
    background: url(../images/line.png) 0 0 no-repeat;
}

.gdu-page-num-info .gdu-curNum {
    position: absolute;
    width: 45px;
    text-align: right;
    left: 10px;
    top: 3px;
    color: #444444;
    font: 36px/1.5 "Microsoft YaHei", "Comic Sans MS", "Verdana";
}

.gdu-page-num-info .gdu-allNum {
    position: absolute;
    left: 58px;
    top: 55px;
    color: #cfcfcf;
    font-weight: bold;
}

.gdu-introduction {
    font-size: 14px;
    line-height: 2;
    margin: 30px 0 0 0;
    overflow: auto;
}


/* 相册文章页码 */

.gdu-swiper-pagination-special {
    height: auto;
    line-height: 10px;
    color: #fff;
    background-color: #666;
}

.gdu-swiper-pagination-special .swiper-pagination-bullet {
    position: relative;
    /*width: 50px;*/
    height: 10px;
    border: none;
    border-radius: 0;
    text-align: center;
    font-size: 10px;
    font-family: sans-serif;
    color: #fff;
    opacity: 1;
    background-color: #666;
}

.gdu-swiper-pagination-special .swiper-pagination-bullet i {
    position: absolute;
    right: 0;
    top: 0;
    height: 10px;
    display: block;
    width: 1px;
    line-height: 0;
    background: #000;
}

.gdu-swiper-pagination-special .swiper-pagination-bullet-active {
    color: #fff;
    background: #e90010;
}

@media (min-width: 768px) and (max-width: 991px) {
    .gdu-special-slide-cover-img {
        left: 80px;
        width: 350px;
    }
    .gdu-special-slide-content {
        left: 460px;
        right: 50px;
    }
}

@media (min-width: 992px) and (max-width: 1199px) {
    .gdu-special-slide-cover-img {
        left: 80px;
    }
    .gdu-special-slide-content {
        left: 580px;
        right: 100px;
    }
}


/* 底部 */

.footer {
    height: 50px;
    line-height: 50px;
    border-top: 1px solid #ddd;
    text-align: center;
    color: #888888;
}

.footer p {
    margin: 0;
    padding: 0;
}

@media(max-width: 767px) {
    .footer {
        font-size: 12px;
        line-height: 25px;
    }
}


/* 回到顶部 */

.go-top {
    width: 55px;
    height: 55px;
    position: fixed;
    background-image: url(../images/gotop.png);
    background-repeat: no-repeat;
    background-position: 0 0;
    bottom: 46px;
    right: 50%;
    margin-right: -456px;
    cursor: pointer;
    display: none;
}

.go-top-hover {
    background-position: 0 -57px;
}
