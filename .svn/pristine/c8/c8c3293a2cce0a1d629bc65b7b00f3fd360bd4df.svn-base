a {
  text-decoration: none;
}
body {
  padding: 0;
  margin: 0;
}
.tmm-div-clear:after {
  display: block;
  content: "";
  clear: both;
  height: 0;
  overflow: hidden;
  visibility: hidden;
}

.bgFilter {
  height: 100%;
  width: 100%;
  background-color: #000;
  opacity: 0.75;
  z-index: 1;
  position: fixed;
  top: 0;
  bottom: 0;
}
.coupon {
  position: fixed;
  top: 0;
  z-index: 2;
  color: #fff;
  width: 100%;
  .img {
    margin-top: 50px;
    margin-left: 50px;
    width: 24px;
    height: 24px;
  }

  .regSuc {
    width: 250px;
    margin:0 auto;
    margin-top: 118px;
  }
  .voteSuc {
    width: 250px;
    margin:0 auto;
    margin-top: 156px;
  }
  .infoSuc {
    .state {
      background-color: #fff;
      height: 37px;
      line-height: 37px;
      text-align: center;
      color: #ea5336;
      font-size: 15px;
      display: block;
      border-radius: 5px;
      font-weight: bold;
    }
    .regState {
      background-color: #fff;
      height: 75px;
      text-align: center;
      display: block;
      border-radius: 5px;
      .info {
        color: #ea5336;
        font-size: 15px;
        line-height: 15px;
        font-weight: bold;
        padding-top: 17px;
        padding-bottom: 10px;
      }
      .share {
        font-size: 13px;
        line-height: 13px;
        color: #000;
      }
    }
    .txt {
      color: #fff;
      font-size: 24px;
      line-height: 24px;
      margin-top: 20px;
      margin-bottom: 10px;
      text-align: center;
    }
    .give-card {
      display: flex;
      width: 100%;
      height: 80px;
      background-color: #fff;
      margin: 0 auto;
      box-shadow: 0 0 2px rgba(0, 0, 0, 0.2);
      .left {
        flex: 1;
        display: flex;
        position: relative;
        align-items: center;
        background: url("../images/dotline.png") repeat-y right;
        background-size: 1px 4px;
        padding-left: 10px;
        padding-right: 10px;
        .price {
          color: #ea5336;
          span {
            font-size: 30px;
            font-weight:bold;
          }
        }
        .intro {
          margin-left: 12px;
          line-height: 1.2em;
          .txt {
            font-size: 13px;
            color: #000;
          }
        }
        .coupons {
          position: absolute;
          bottom: 7px;
          right: 10px;
          font-size: 10px;
          color: #ea5336;
        }
      }
      .right {
        overflow: hidden;
        width: 76px;
        height: 100%;
        background-color: #ea5336;
        color: #fff;
        text-align: center;
        a {
          display: inline-block;
          margin-top: 20px;
          font-size: 16px;
          line-height: 20px;
          color: #fff;
        }
      }
    }
  }
}