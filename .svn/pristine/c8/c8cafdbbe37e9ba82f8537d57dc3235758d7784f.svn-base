<!-- 我发起的活动 -->
<ion-view view-title="我发起的活动" cache-view="false" hide-tabs>
  <ion-nav-buttons>
    <i class="tmm-ion-left ion-android-arrow-back" ui-sref="tab.my" nav-direction="back"></i>
    <i class="tmm-ion-right ion-android-add" ng-click="showPopup()" ></i>
  </ion-nav-buttons>
  
  <ion-content class="tmm-mylaunch-act-content">
	  
	  <div class="tmm-activity-card-list">
	    <div class="card tmm-my-activity-card">
	      <a href=""  data-link="{{link}}" ui-sref="tab.launch-actdeta" class="tmm-my-act-state-detail" data-type="{{js " this.type==1 ? 'actSuccess' : 'actFail' "}}" >
	        <div class="card-content">
	          <div class="tmm-activity-img"><img src=""></div>
	          <div class="tmm-activity-content">
	            <div class="tmm-activity-content-title">这是一个完整的活动</div>
	            <div class="tmm-activity-content-info">
	            	<span class="tmm-activity-content-count">出游日期：待定</span>
	              <p>参与人数：12/人</p>
	              <!--<span class="tmm-activity-content-count">出游日期：{{go_time}}</span>
	              <p>下单人数：{{actives_info.order_count.value}}</p>-->
	            </div>
	          </div>
	          <div class="tmm-activity-status">
	          	
	          <span class="tmm-icon-triangle tmm-tip-audit-failure" title="{{audit_status.name}}">审核未通过</span>
	            <!--<span class="tmm-icon-triangle tmm-tip-audit-failure" title="{{audit_status.name}}">{{audit_status.name}}</span>
	            <span class="tmm-icon-triangle tmm-tip-gray" title="{{actives_status.name}}">{{actives_status.name}}</span>
	            <span class="tmm-icon-triangle tmm-tip-apply" title="{{actives_status.name}}">{{audit_status.name}}</span>
	            <span class="tmm-icon-triangle tmm-tip-apply" title="{{actives_status.name}}">{{actives_status.name}}</span>
	            <span class="tmm-icon-triangle tmm-tip-gray" title="{{actives_status.name}}">{{actives_status.name}}</span>
	            <span class="tmm-icon-triangle tmm-tip-gray" title="{{actives_status.name}}">{{actives_status.name}}</span>-->
	          </div>
	        </div>
	      </a>  
	        <div class="card-footer no-border">
	          <div class="tmm-card-footer tmm-my-activity-b-border">
	            <!-- <a href="#" class="button">删除</a> -->
	            <a href="" class="button tmm-act-edit" data-link="{{link}}" data-type="{{tour_type.value}}">编辑</a>
	            <!--<a href="" class="button tmm-my-activity-cancel">取消</a>
	            <a href="" class="button activityGoTimeSel" data-id="{{value}}" data-type="activityGoTimeSel" style="width: 110px;" id="goTime_{{value}}" data-link="{{set_go_time.link}}">确认出游日期</a>
	          	<a href="" class="button tmm-my-activity-cancel" style="width: 90px;">取消活动</a>-->
	          </div>
	        </div>
	    </div>
    </div>

  </ion-content>
</ion-view>


