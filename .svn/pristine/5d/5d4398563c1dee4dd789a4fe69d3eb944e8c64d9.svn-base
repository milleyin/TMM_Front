/***************************************************************************/
/**
 * login
 */
.tmm-login-page {
  background: #3f3936;
}
.tmm-login-page .close {
  display: block;
  position: absolute;
  top: 16px;
  right: 16px;
}
.tmm-login-page .close .icon {
  color: #d7d9d7;
  font-size: 20px;
}
.tmm-login-page .tmm-login {
  position: fixed;
  left: 0px;
  bottom: 0px;
  background-color: #c8a063;
  width: 100%;
  height: 48px;
  text-align: center;
  line-height: 48px;
}
.tmm-login-page .tmm-login a {
  position: relative;
  display: block;
  float: left;
  width: 50%;
  color: #111;
  font-size: 15px;
  border-radius: 0;
}
.tmm-login-page .tmm-login a.tmm-acc-login:after {
  content: '';
  display: block;
  position: absolute;
  width: 1px;
  height: 26px;
  top: 11px;
  right: 0px;
  background-color: #403a39;
}
.tmm-login-page .tmm-bg-img {
  width: 100%;
  height: 100%;
  background: #3f3936 url(../images/logo1.png) no-repeat 0 center;
  background-size: 100%;
}
/**
 * acc-login
 */
.tmm-acc-login-page {
  background: #3f3936;
}
.tmm-acc-login-page .tmm-acc-icon {
  margin: 10px 0 0 0;
}
.tmm-acc-login-page .tmm-acc-icon .icon {
  font-size: 20px;
  color: #d7d9d7;
}
.tmm-acc-login-page .list-block {
  margin: 18px 0;
}
.tmm-acc-login-page .list-block ul {
  border-radius: 4px;
}
.tmm-acc-login-page .list-block .item-content {
  margin: 0 16px;
  padding-left: 0px;
}
.tmm-acc-login-page .list-block.inset {
  margin-left: 15px;
  margin-right: 15px;
}
.tmm-acc-login-page .tmm-showMsg-li {
  display: none;
}
.tmm-acc-login-page .tmm-showMsg-li .tmm-showMsg {
  color: #ff595f;
}
.tmm-acc-login-page .item-input input {
  font-size: 14px;
}
.tmm-acc-login-page .item-input .tmm-verifycode-img {
  width: 107px;
  height: 30px;
  position: absolute;
  right: 0;
  top: 7px;
}
.tmm-acc-login-page .item-input .get-code {
  text-align: center;
  background-color: #ff595f;
  font-size: 14px;
  line-height: 30px;
  color: #fff;
  width: 107px;
  height: 30px;
  position: absolute;
  right: 0;
  top: 7px;
}
.tmm-acc-login-page .tmm-disable-login {
  margin-top: 20px;
  text-align: center;
}
.tmm-acc-login-page .tmm-disable-login a {
  color: #ffffff;
}
.tmm-acc-login-page .tmm-login-submit {
  background-color: #ff595f;
  border-radius: 4px;
  border: none;
  color: #fff;
}
/**
 * 我的信息页面
 */
.tmm-myinfo-page-contnet .tmm-myinfo-content-otherretinue .content-block-inner {
  font-size: 0px;
}
.tmm-myinfo-page-contnet .tmm-myinfo-content-otherretinue .content-block-inner span {
  width: 33%;
}
.tmm-myinfo-page-contnet .tmm-myinfo-content-otherretinue .content-block-inner span:nth-child(3n-2) {
  text-align: left;
}
.tmm-myinfo-page-contnet .tmm-myinfo-content-otherretinue .content-block-inner span:nth-child(3n-1) {
  text-align: center;
}
.tmm-myinfo-page-contnet .tmm-myinfo-content-otherretinue .content-block-inner span:nth-child(3n) {
  text-align: right;
}
.tmm-myinfo-page-contnet .tmm-myinfo-item-info .item-after .side_txt {
  color: #858b8f;
}
.tmm-myinfo-page-contnet .tmm-myinfo-item-info .item-after .icon {
  font-size: 14px;
  color: #d7d9d7;
}
/**
 * 修改手机号页面
 */
.disable {
  background-color: #d7d9d7 !important;
}
.set-old-phone-wrap,
.set-new-phone-wrap {
  position: absolute;
  width: 100%;
  left: 0;
  top: 0;
  transition: all .3s;
  -webkit-transiton: all .3s;
}
.set-new-phone-wrap {
  left: 100%;
}
/**
 * 我的消息页面
 */
.tmm-my-msg {
  background: url(../images/wodexiaoxi.png) no-repeat 0 center;
  background-size: 100%;
}
/**
 * 设置弹出层样式
 */
#tmm-my-info {
  width: 58px;
}
#tmm-my-info .item-media {
  width: 58px;
}
/*我的首页*/
#myView a#tmm-my-info,
#myView a#tmm-my-order,
#myView a#tmm-my-msg,
#myView a#tmm-my-praise
{
  color: #313131;
}
