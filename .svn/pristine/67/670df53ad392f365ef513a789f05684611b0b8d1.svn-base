<ion-view title="服务费" hide-tabs="true">
  <ion-nav-buttons side="left">
    <i class="icon-back" nav-back="back"></i>
  </ion-nav-buttons>

  <ion-content>
    <div class="tmm-edit-act">
      <div class="edit-title">
        <input required class="content-txt num-width" type="text" placeholder="{{tourprice}}" ng-model="model.edittourprice">&nbsp;&nbsp;元
      </div>
      
      <span class="card-btn" ng-click="editTourPrice()">完成</span>
    </div>
  </ion-content>
</ion-view>
