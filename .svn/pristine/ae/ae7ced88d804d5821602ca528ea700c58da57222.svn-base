<ion-view title="觅趣参与人数" hide-tabs="true">
  <ion-nav-buttons side="left">
    <i class="icon-back" nav-back="back"></i>
  </ion-nav-buttons>

  <ion-content>
    <div class="tmm-edit-act">
      <div class="edit-title">
        <input required class="content-txt num-width" type="text" placeholder="{{num}}" ng-model="model.editactnum">&nbsp;&nbsp;人
      </div>
      
      <span class="card-btn" ng-click="editNum()">完成</span>
    </div>
  </ion-content>
</ion-view>
