/**
 * 我的 模块的样式
 * 
 */

.tmm-ion-left{font-size: 30px;margin-left: 10px;color: #ff595f;}
.tmm-ion-right{position: absolute;right: 0px;font-size: 30px;margin-right:10px ;color: #ff595f;}

.tmm-my-order-b-border, .tmm-day-num, .tmm-item-li{
  position: relative;
}

.tmm-my-order-b-border:before, .tmm-item-li:before{
  content: '';
  position: absolute;
  left: 0;
  bottom: auto;
  right: 0;
  top: 0;
  height: 1px;
  width: 100%;
  background-color: #d7d9d7;
  display: block;
  z-index: 15;
  -webkit-transform-origin: 50% 100%;
  transform-origin: 50% 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.tmm-retinue-top-border, .tmm-retinue-bottom-border{position: relative;}
.tmm-retinue-top-border:before{ /*上面边框*/
	top:0;
}
.tmm-retinue-bottom-border:after{ /*下面边框*/
	bottom:0;
}
.tmm-retinue-top-border:before, .tmm-retinue-bottom-border:after{
	position: absolute;
    content: "";
    height: 1px;
    background-color:#d7d9d7;
    left: 0;
    right: 0;
	-webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
	-webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;	
}

.tmm-right{
	font-size: 13px !important;
    color: #ff595f;
}


