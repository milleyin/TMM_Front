<div class="navbar tmm-navbar">
  <div class="navbar-inner tmm-trade-record-inner">
    <div class="left">
      <a href="javascript:history.back(-1);">
        <i class="iconfont ticon-arrow-back">&#xe61f;</i>
      </a>
    </div>
    <div class="center sliding">提现申请记录</div>
    <div class="right"></div>
  </div>
</div>
<div class="pages">
  <div data-page="draw-money-record" class="page tmm-withdrawal-application-record-page" id="tmm-draw-money-record-page">
    <div class="page-content" when-scrolled="showMore()">
      
      <!--提现申请记录-->
      <div class="tmm-recor-canter">
        <div class="tmm-pre-canter tmm-fin-con">
          <div class="list-block media-list tmm-list-block">
            <ul id="tmm-draw-money-record-ul" class="tmm-record-conNo-ul">
              <li class="item-content" ng-repeat="drawRecord in list_data">
                <div class="item-inner">
                  <div class="item-title-row">
                    <div class="item-title list-block-font">
                      <b>提现</b>
                      <p class="tmm-time-reason">{{drawRecord.add_time}}</p>
                    </div>
                    <div class="item-after color-gre">{{drawRecord.price}}   
                      <p class="suc" ng-if="drawRecord.audit_status.value == 3">提现成功</p>
                      <p class="fail" ng-if="drawRecord.audit_status.value == 2">打款中...</p>
                      <p class="fail" ng-if="drawRecord.audit_status.value == 0 || drawRecord.audit_status.value == 1">审核中...</p>
                      <p class="fail" ng-if="drawRecord.audit_status.value == -1 ||drawRecord.audit_status.value == -2 || drawRecord.audit_status.value == -3">提现失败</p>
                    </div>
                  </div>
                  
                  <div class="item-text tmm-time-reason" ng-if="drawRecord.cash_status.value == -1">原因：{{drawRecord.remark_first}}</div>
                  <div class="item-text tmm-time-reason" ng-if="drawRecord.cash_status.value == -2">原因：{{drawRecord.remark_double}}</div>
                  <div class="item-text tmm-time-reason" ng-if="drawRecord.cash_status.value == -3">原因：{{drawRecord.remark_submit}}</div>
                    
                </div>
              </li>
              <!-- 暂无记录 -->
              <li class="tmm-record-content tmm-record-conNo" ng-if="listNum == 0">
                你暂时还没有提现申请记录哦！
              </li>

            </ul>
          </div>
        </div>
        <div class="tip" style="display:none;">正在加载...</div>
      </div>
    </div>
  </div>
</div>
