  position: absolute;
  top: 0;
  right: 0;
  height: 50px;
  width: 50px;
}
.card.tmm-my-activity-card .card-content .tmm-activity-status .tmm-icon-triangle{
  position: absolute;
  top: -4px;
  right: -20px;
  width: 60px;
  height: 12px;
  font-size: 9px;
  text-align: center;
  padding-top: 15px;
  color: #fff;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
}
.card.tmm-my-activity-card .card-content .tmm-activity-status .tmm-icon-triangle.triangle_5{
  position: absolute;
  top: -10px;
  right: -30px;
  width: 80px;
  height: 30px;
  font-size: 9px;
  text-align: center;
  padding-top: 15px;
  color: #fff;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
}
.card.tmm-my-activity-card .card-content .tmm-activity-status .tmm-icon-triangle p{
  position: absolute;
  bottom: 0px;
  left: 10px;
}
.card.tmm-my-activity-card .card-content .tmm-activity-status .tmm-tip-gray{background-color: #858b8f;}
.card.tmm-my-activity-card .card-content .tmm-activity-status .tmm-tip-audit-failure {background-color: #ff0054;}
.card.tmm-my-activity-card .card-content .tmm-activity-status .tmm-tip-chuli {background-color: #00aaff;}
.card.tmm-my-activity-card .card-content .tmm-activity-status .tmm-tip-apply {background-color: #9f59ff; }
.card.tmm-my-activity-card .card-content .tmm-activity-status .tmm-tip-xiaofei {background-color: #14cc14;}

.card.tmm-my-activity-card .card-footer {
  padding: 0 10px;
}

.card.tmm-my-activity-card .tmm-card-footer {
    width: 100%;
    padding: 10px 0;
    text-align: right;
}

.card.tmm-my-activity-card .card-footer a.button {
