<div class="pages navbar-through">
  <div data-page="mob-login" class="page no-navbar no-toolbar">
    <div class="page-content tmm-acc-login-page">
      <div class="content-block tmm-acc-icon">
        <div class="row">
          <div class="col-100">
            <a href="#" class="back link">
              <i class="icon ticon-arrow-back"></i>
            </a>
          </div>
        </div>
      </div>
      <div class="list-block inset">
        <ul>
          <li class="tmm-showMsg-li" style="display:none;">
            <div class="item-content">
              <div class="item-inner">
                <div class="item-input">
                  <div class="tmm-showMsg"></div>
                </div>
              </div>
            </div>
          </li>
          <li>
            <div class="item-content">
              <div class="item-inner">
                <div class="item-input">
                  <input type="tel" id="tmm-mob-phone" value="" placeholder="请输入手机号" maxlength="30">
                </div>
              </div>
            </div>
          </li>
          <li>
            <div class="item-content">
              <div class="item-inner">
                <div class="item-input tmm-item-input-img">
                  <input type="text" id="tmm-mob-verifycode" placeholder="请输入图形验证码" maxlength="30">
                  <img class="tmm-verifycode-img">
                </div>
              </div>
            </div>
          </li>
          <li>
            <div class="item-content">
              <div class="item-inner">
                <div class="item-input">
                  <input type="text" id="tmm-mob-sms" placeholder="请输入短信验证码" value="" maxlength="30">
                  <span class="get-code">获取验证码</span>
                </div>
              </div>
            </div>
          </li>
        </ul>
      </div>
      <div class="content-block">
        <div class="row">
          <div class="col-100">
            <a href="javascript:;" id="tmm-mob-login-submit" class="button button-big active tmm-login-submit">
              登录
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
