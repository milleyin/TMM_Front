// bower:scss
// endbower
@import "compass/reset";
@import "./components/mixins";
@import "./components/extend";
@import "./components/common";
body,
html {
  height: 100%;
  font-family: Helvetica, \\534E\6587\7EC6\9ED1;
}

.container {
  height: auto;
}

.page {
  height: 100%;
  display: flex;
  flex-direction: column;
}

.navbar {
  height: 44px;
  line-height: 44px;
  display: flex;
  justify-content: space-between;
  @extend %border-bottom;
  .title {
    font-size: 18px;
  }
  .left, .right {
    width: 44px;
  }
}

.toolbar {
  height: 44px;
  line-height: 44px;
  @extend %border-top;
  display: flex;
}

.content {
  width: 100%;
  box-sizing: border-box;
  flex: 1;
  overflow-y: auto;
  overflow-x: hidden;
}

.active {
  .toolbar {
    justify-content: center;
    background-color: rgba(59, 158, 219, .9);
    color: #fff;
  }
  .banner {
    // width: 100%;
    // overflow: hidden;
    img {
      width: 100%;
      display: block;
    }
  }
  .intro {
    width: 100%;
    overflow: hidden;
    .box:nth-child(2n+1) {
      @extend %border-right-bottom;
    }
    .box:nth-child(2n) {
      @extend %border-bottom;
    }
    .box {
      float: left;
      display: flex;
      flex-direction: column;
      padding-left: 10px;
      height: 80px;
      justify-content: center;
      &.col-12 {
        height: auto;
        overflow: hidden;
        padding: 0;
        .box-tit {
          line-height: 44px;
          padding-left: 10px;
        }
      }
    }
    .active-intro {
      padding: 10px 10px 30px;
      color: #666;
    }
    .box-tit {
      font-weight: 600;
      font-size: 15px;
      color: #333;
    }
    .box-txt {
      font-size: 13px;
      color: #666;
      margin-top: 15px;
    }
  }
}

.shop-wrap {
  padding: 30px 14px;
  text-align: center;
  .schedule {
    text-align: center;
  }
  .schedule-day {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    margin: 15px;
    height: 90px;
    background-repeat: no-repeat;
    background-image: linear-gradient(180deg, transparent 9px, #3b9edb 9px, #3b9edb 86px, transparent 100%);
    background-position: 50%;
    background-size: 1px 100%;
    .day {
      padding: 4px 6px;
      line-height: 11px;
      font-size: 11px;
      background-color: #3b9edb;
      color: #fff;
      border-radius: 4px;
    }
    .circle {
      width: 7px;
      height: 7px;
      border-radius: 100%;
      border: 1px solid #3b9edb;
    }
    .arrow {
      width: 0;
      height: 0;
      border-left: 3px solid transparent;
      border-right: 3px solid transparent;
      border-top: 5px solid #3b9edb;
    }
  }
  .item-title {
    font-size: 20px;
    font-weight: bold;
    margin: 20px 0;
  }
  .item-address {
    font-size: 14px;
    color: #666;
  }
  .img {
    margin-top: 20px;
    margin-bottom: 10px;
    img {
      width: 100%;
      height: auto;
    }
  }
}

.select-people {
  padding-left: 14px;
  .cell {
    display: flex;
    justify-content: space-between;
    padding-right: 14px;
  }
  .num {
    margin: 0 8px;
  }
  .sub-num {
    display: inline-block;
    vertical-align: middle;
    height: 24px !important;
    width: 24px !important;
    position: relative;
    border: 1px solid #d7d9d7;
    border-radius: 24px;
  }
  .sub-num:before {
    content: '';
    height: 2px;
    width: 12px;
    display: block;
    background: #d7d9d7;
    position: absolute;
    top: 11px;
    left: 6px;
  }
  .add-num {
    display: inline-block;
    vertical-align: middle;
    height: 24px !important;
    width: 24px !important;
    position: relative;
    border: 1px solid #d7d9d7;
    border-radius: 24px;
  }
  .add-num:before,
  .add-num:after {
    content: '';
    height: 2px;
    width: 12px;
    display: block;
    background: #ff595f;
    position: absolute;
    top: 11px;
    left: 6px;
  }
  .add-num:after {
    height: 12px;
    width: 2px;
    top: 6px;
    left: 11px;
  }
}

.group {
  @extend %border-top-bottom;
}

.cell {
  height: 44px;
  line-height: 44px;
  @extend %border-bottom;
  &:nth-last-child {
    background-image: none;
  }
}

.title-block {
  padding: 35px 14px 10px;
  line-height: 13px;
  font-size: 13px;
  color: #6d6d72;
  background-color: #f0f1f2;
}

.block-people {
  .group {
    padding-left: 14px;
    input {
      border: none;
      font-size: 16px;
      width: 200px;
      line-height: 30px;
    }
    .error-msg {
      font-size: 12px;
      color: #ff4c52;
      margin-left: 10px;
    }
    .cell {
      display: flex;
      align-items: center;
    }
    .code-cell {
      justify-content: space-between;
    }
    .code {
      display: block;
      width: 88px;
      height: 30px;
      background-color: #ff4c52;
      color: #fff;
      font-size: 14px;
      line-height: 30px;
      text-align: center;
      margin-right: 14px;
    }
  }
}

.apply {
  height: auto;
  position: relative;
  margin-top: 44px;
  .navbar {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    background-color: #fff;
    z-index: 1;
  }
  .submit {
    height: 35px;
    line-height: 35px;
    margin: 16px 30px;
    border-radius: 5px;
    background-color: #ff4c52;
    text-align: center;
    color: #fff;
  }
}

.statusbar {
  height: 30px;
  line-height: 30px;
  background-color: rgba(0, 0, 0, 0.85);
  text-align: center;
  color: #fff;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 100;
  font-size: 14px;
  transition: all .25s ease;
}

.std-begin-enter {
  transform: translate3d(0, -100%, 0);
}

.std-end-enter {
  transform: translate3d(0, 0, 0);
}

.has-error {
  outline: 1px solid #ff4c52;
}

.code.gray {
  background-color: #ccc !important;
}

.mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 10;
}

.alertmodal {
  display: block;
  position: fixed;

  overflow: hidden;
  top: 50%;
  left: 50%;
  width: 280px;
  margin-left: -140px;
  margin-top: -100px;
  z-index: 100;
  background-color: #fff;
  text-align: center;
  border-radius: 5px;
  border: 4px solid rgba(0, 0, 0, 0.5);
  .title {

    width: 100%;
    line-height: 24px;
    padding: 10px 0;
    @extend %border-bottom;
  }
  .confirm,
  .cancel {
    float: left;
    width: 50%;
    line-height: 49px;
  }
  .cancel {
    @extend %border-right;
  }
}

.loadapp {
  font-size: 16px;
  display: block;
  text-align: center;
  background-color: #ff4c52;
  color: #fff;
  line-height: 36px;
  margin: 50px 20px;
  border-radius: 5px;
}

.arrow-back {
  height: 44px;
  width: 44px;
  background-image: url(data:image/png;base64,);
}

