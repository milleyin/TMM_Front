@charset "UTF-8";
.tmm-record {
  .record-list {
    margin-left: 14px;
    padding-right: 14px;
    height: 50px;
    @extend %border-bottom; 
  }
  .record-conNo {
    padding-top: 35px;
    text-align: center;
    line-height: 48px;
    color: #403a39;
  }
  .income {
    font-size: 16px;
    color: #000;
    display: flex;
    justify-content: space-between;
    padding-top: 8px;
  }
  .balance {
    font-size: 12px;
    color: #8e8e93;
    line-height: 12px;
    display: flex;
    justify-content: space-between;
  }
}


