.tmm-hotel-content{
  text-align: left;
  display: inline-block;
  margin: 0px 25px 17px 0;
}
/********************************************/

/**
 * 搜索样式
 */
.tmm-search-img { top: 10px;}
.tmm-search-left { z-index: 10;}
.tmm-item-input-inset {
  margin-left: 45px;
  width: 100%;
}
.tmm-search-input { width: 100%;}
.tmm-search-content { background: #fff;}
.tmm-button-clear { font-size: 15px; }
.tmm-search-list { 
  margin-left: 16px; 
  margin-top: -1px;
  margin-bottom: 0px;
}
.tmm-search-item{ 
  padding: 10px 0px; 
  font-size: 15px; 
  color: #565a5c;
  border-left: 0;
}
.tmm-history{
  font-size: 13px;
  color: #8f8585;
}
.tmm-history-li {
  max-height: 250px;
  overflow-y: auto;
  overflow-x: hidden;
}
.tmm-search-clear{ 
  padding: 10px 0px; 
  font-size: 13px; 
  color: #ff595f;
  border-left: 0;
  text-align: center;
}
/********************************************/

/**
 * 点订单预订
 */
.tmm-order-book-title {
  position: relative;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 13px;
  text-transform: uppercase;
  line-height: 1;
  color: #6d6d72;
  margin: 35px 15px 10px;
}
.tmm-order-book-time {
  margin:10px 0 35px 0;
  background-color: #FFFFFF;
  font-size: 15px;
  line-height: 25px;
  color: #565a5c;
  padding: 10px 15px;
}
.tmm-list{
  margin-bottom: 10px;
}
.tmm-orderbook-list .list {
  padding-left: 15px;
  background-color: #fff;
}

.tmm-orderbook-list .item { 
  padding-left: 0;
  border-left: none;
  border-right: none;
}
.tmm-orderbook-item{
  display: inline-flex;
  width: 100%;
}
.tmm-orderbook-list .item:first-child {
  border-top: none;
}
.tmm-orderbook-list .item:last-child {
  border-bottom: none;
}
.tmm-orderbook-item-title {
  color: #565a5c;
  font-size: 15px;
}
.tmm-orderbook-item-content{
  font-size: 14px;
  color: #858b8f;
}
.tmm-orderbook-item-content .tmm-item-element {
  vertical-align: middle;
}
.tmm-orderbook-item-content .tmm-item-element:nth-child(1){
  width: 30%;
  word-break: break-all;
  word-wrap: break-word;
  text-overflow: ellipsis;
  white-space: normal;
  display: inline-block;
} 
.tmm-orderbook-item-content .tmm-item-element:nth-child(2){
  width: 15%;
  text-align: center;
} 
.tmm-orderbook-item-content .tmm-item-element:nth-child(3){
  width: 25%;
  text-align: center;
} 
.tmm-orderbook-item-content .tmm-item-element:nth-child(4){
  width: 30%;
  text-align: right;
} 
.tmm-sub-num {
  display: inline-block;
  vertical-align: middle;
  height: 24px;
  width: 24px;
  position: relative;
  border: 1px solid #d7d9d7;
  border-radius: 24px;
}
.tmm-sub-num:before {
  content: '';
  height: 2px;
  width: 12px;
  display: block;
  background: #d7d9d7;
  position: absolute;
  top: 10px;
  left: 5px;
}
.tmm-add-num {
  display: inline-block;
  vertical-align: middle;
  height: 24px;
  width: 24px;
  position: relative;
  border: 1px solid #d7d9d7;
  border-radius: 24px;
}
.tmm-add-num:before {
  content: '';
  height: 2px;
  width: 12px;
  display: block;
  background: #ff595f;
  position: absolute;
  top: 10px;
  left: 5px;
}
.tmm-add-num:after {
  content: '';
  height: 12px;
  width: 2px;
  top: 5px;
  left: 10px;
  display: block;
  background: #ff595f;
  position: absolute;
}
.tmm-order-hoteltime {
  height: 23px !important;
  margin-top: -15px;
  margin-bottom: 10px;
}
.tmm-order-retinue { 
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-lines: multiple;
  -moz-box-lines: multiple;
  -webkit-flex-wrap: wrap;
  -webkit-justify-content: space-between; 
  justify-content: space-between; 
  bottom: 35px;
  position: relative;
}
.tmm-w-33 {
  width: 33.3%;
}
.tmm-retinue-sel{
  color: #ff595f;
}
.tmm-order-btn {
  height: 50px;
  line-height: 50px;
  background-color: #fff;
  text-align: center;
  position: absolute;
  width: 100%;
  left: 0px;
  bottom: 0px;
  z-index: 100;
}
.tmm-order-price {
  color: #ff595f;
  font-size: 20px;
  float: left;
  margin-left: 11px;
}
.tmm-order-confirm-btn {
  display: inline-block;
  width: 160px;
  height: 33px;
  font-size: 15px;
  color: #fff;
  line-height: 33px;
  border-radius: 2px;
  background: #ff595f;
  float: right;
  margin-top: 8px;
  margin-right: 16px;
}
/**
 * 线订单预订
 */
.tmm-order-addsub {
  display: flex;
}
.tmm-order-addsub div { width: 50%;}
.tmm-addsub-pos {text-align: right;}
.tmm-type-icon {
  font-size: 20px;
  margin-right: 12px;
}
.tmm-day-time {
  height: 45px;
  background-color: #fff;
  text-align: center;
  position: relative;
}
.tmm-day-list-title {
  margin-left: -15px;
}
/********************************************/