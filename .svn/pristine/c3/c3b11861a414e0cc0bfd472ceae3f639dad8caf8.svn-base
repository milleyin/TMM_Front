/*
 * 活动详情内容css
 */
.wrap{
  padding: 0 16px;
  text-align: center;
}
.wrap .info{
  text-align: center;
  padding-bottom: 26px;
}
.wrap .schedule{
  margin-top: 30px;
  position: relative;
  font-size: 16px;
  padding-bottom: 10px;
  color: #616466;
  word-wrap: break-word;
}
.wrap .bracket {
  font-size: 16px;
  -webkit-text-fill-color: white;
  -webkit-text-stroke-color: #000;
  -webkit-text-stroke-width: 0.5px;
  text-fill-color: white;
  text-stroke-color: #000;
  text-stroke-width: 0.5px;
  font-style: initial;
}
.wrap .schedule-detail{
  position: relative;
}
.wrap .schedule-detail .bg {
  height: 86px;
  position: absolute;
  top: 0px;
  right: 0;
  width: 100%;
  z-index: 1;
}
.wrap .bg .bg_o {
  margin: 0 auto;
  width: 6px;
  height: 6px; 
  border-radius: 8px;
}
.wrap .bg .bg_o_line {
  border: 1px solid #39ab53;
}
.wrap .bg .bg_o_act {
  border: 1px solid #3b9edb;
}

.wrap .bg .bg_l {
  position: absolute;
  left: 50%;
  height: 78px;
  width: 1px;
  margin: 0 auto;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
  -webkit-transform-origin: 0 50%;
  transform-origin: 0 50%;
}
.wrap .bg .bg_l_line {
  background-color: #39ab53;
}
.wrap .bg .bg_l_act {
  background-color: #3b9edb;
}
.wrap .schedule-detail .day {
  height: 60px;
  padding-top: 34px;
  position: relative;
  z-index: 1;
}
.wrap .day .day-content{
  padding: 4px 6px;
  background-color: #3b9edb;
  color: #fff;
  font-size: 11px;
  border-radius: 4px;
  display: inline-block;
  line-height: 11px;
}
.wrap .day .day-content-line {
  background-color: #39ab53;
}
.wrap .day .day-content-act {
  background-color: #3b9edb;
}
.wrap .tmm-ticon-point {
  position: absolute;
  margin-left: -3px;
  margin-top: 6px;
  left: 50%;
}
.wrap .tmm-ticon-point:after, .wrap .tmm-ticon-point:before {
  border: 3px solid transparent;
  border-top: 3px solid #fff;
  width: 0;
  height: 0;
  position: absolute;
  left: 0;
  bottom: -0px;
  content: ' ';
}
.wrap .tmm-ticon-point:before {
  border-top-color: #3b9edb;
  bottom: -1px;
}
.tmm-line-ticon-point:before {
  border-top-color: #39ab53 !important;
}
.wrap .tmm-content-pos-top{
  margin-top: 30px; 
  padding-bottom: 26px;
  border-bottom: 1px solid #d7d9d7;
}
.wrap .tmm-img-pos {
  position: relative;
  background-repeat: no-repeat;
  margin: 0px auto;
}
.wrap .tmm-schedule-detail-title {
  margin-top: 10px;
  font-size: 20px;
  line-height: 20px;
  margin: 15px 0px 12px;
  font-weight: bold;
  color: #000000;
}
.wrap .tmm-line-address {
  font-size: 14px;
  color: #666666;
  word-break: break-word;
}
.wrap .tmm-showmore {
  display: inline-block;
  margin: 20px 0px 25px 0px;
  font-size: 8px;
  background-color: #fff;
  color: #ff595f;
  height: 8px;
  line-height: 8px;
}
.wrap .tmm-showmore span:nth-child(2) {
    padding: 0px 5px;
}
.wrap .img{ width: 100%; vertical-align: middle;  }


