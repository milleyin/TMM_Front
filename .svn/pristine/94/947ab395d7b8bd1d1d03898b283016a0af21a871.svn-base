<script type="text/javascript">
	$(function(){
		/*alert("IE不支持find？？");*/
		/*我的分成 的样式*/
	  $("table").find("td").each(function(i){ 
			if(i%9 == 5){  
				$(this).attr("class","shared_revenue");
			} 
		});
		/*账号状态 的样式*/
		$("table").find("td").each(function(i){ 
			if(i%9 == 6){  
				$(this).attr("class","state");
			} 
		}); 
		/*启用的按钮*/
		$(".start").click(function(e){
			var $text=$(this).html();
			/*alert($text);*/
			if($text=="启用"){
				$(".popup_div").css("display","block");
				$(".start_div").css("display","block");
				$(".stop_div").css("display","none");
			}else if($text=="禁用"){
				$(".popup_div").css("display","block");
				$(".stop_div").css("display","block");
				$(".start_div").css("display","none");
			}
		})
		/*账号详情的按钮*/
