<ion-view view-title="银行卡管理" hide-tabs >
  <ion-nav-buttons side="left">
    <i class="tmm-ion-left icon icon-arrow-back" nav-back></i>
  </ion-nav-buttons>

  <ion-content class="tmm-bankcard-manage">
    <div class="card-top">
      <img src="images/card1.png"/>
    </div>

    <div class="bankcard-list">
      <div class="bankcard-item">
        <div class="bankcard-title">真实姓名</div>
        <div>{{json.bank_name}}</div>
      </div>
      <div class="bankcard-item">
        <div class="bankcard-title">开户银行</div>
        <div>{{json.bank_value}}{{json.bank_branch}}</div>
      </div>
      <div class="bankcard-item">
        <div class="bankcard-title">银行卡号</div>
        <div>{{json.bank_code_format}}</div>
      </div>
    </div>

    <div class="wall-message-button">
      <button class="message-sub bankcard-btnbg" type="submit" ui-sref="tab.mybankcardchange">更换银行卡</button>
    </div>

  </ion-content>
</ion-view>


