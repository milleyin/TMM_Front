/*弹出框的样式*/
.popup_div{
	position:absolute;
	top: 280px;
	left: 300px;
	/*边框阴影*/
	border: 1px solid rgb(178,178,178);
	-moz-box-shadow:0px 0px 10px #BFBFBF; /* For Firefox3.6+ */ 
	-webkit-box-shadow:0px 0px 10px #BFBFBF; /* For Chrome5+, Safari5+ */ 
	box-shadow:0px 0px 10px #BFBFBF; 
	/*边框阴影*/
	width: 400px;
	height: 300px;
	margin:0px 0px 50px 0px;/*上，右，下，左*/
	z-index: 99;
	/*display: none;/*div隐藏*/*/
}
.popup_content{
	margin:60px 0px 0px 90px;/*上，右，下，左*/
	font-size: 16px;
	margin-bottom: 0px;
}
