{{#if isLogin}}
<!-- 已登录 -->
<div class="list-block media-list tmm-logo-list-block no-border">
  <ul>
    <li>
      <div class="item-content">
        <a href="javascript:;" id="tmm-my-info">
          <div class="item-media">
            <img src="../../styles/images/header.png" />
          </div>
        </a>
        <div class="item-inner">
          <div class="item-title-row">
            <div class="item-title">{{userInfo.phone}}</div>
          </div>
        </div>
        <div class="item-after">
          <a href="#" class="login-out">退出</a>
        </div>
      </div>
    </li>
  </ul>
</div>
{{else}}
<!-- 未登录 -->
<div class="list-block tmm-my-item-info tmm-icon-login">
  <ul>
    <a href="javascript:;" id="tmm-my-login">
      <li class="item-content">
        <div class="item-media"><i class="icon ticon-login"></i></div>
        <div class="item-inner">
          <div class="item-title">请登陆账号</div>
        </div>
      </li>
    </a>
  </ul>
</div>
{{/if}}
<div class="list-block tmm-my-item-info tmm-icon-blue">
  <ul>
    <a href="javascript:;" id="tmm-my-order">
      <li class="item-content">
        <div class="item-media"><i class="icon ticon-order"></i></div>
        <div class="item-inner b-border">
          <div class="item-title">我的订单</div>
          <div class="item-after">
            <i class="icon ticon-arrow-right"></i>
          </div>
        </div>
      </li>
    </a>
    <a href="javascript:;" id="tmm-my-msg">
      <li class="item-content">
        <div class="item-media"><i class="icon ticon-info"></i></div>
        <div class="item-inner">
          <div class="item-title">我的消息</div>
          <div class="item-after">
            <i class="icon ticon-arrow-right"></i>
          </div>
        </div>
      </li>
    </a>
  </ul>
</div>
<div class="list-block tmm-my-item-info tmm-icon-red">
  <ul>
    <a href="javascript:;" id="tmm-my-praise">
      <li class="item-content">
        <div class="item-media"><i class="icon ticon-praise"></i></div>
        <div class="item-inner">
          <div class="item-title">我的赞</div>
          <div class="item-after">
            <i class="icon ticon-arrow-right"></i>
          </div>
        </div>
      </li>
    </a>
  </ul>
</div>
