/**
 * toolbar.tmm-toolbar
 */

.toolbar.tmm-toolbar {
  height: 50px;
  background-color: #403A39;
  opacity: 0.97;
}

.toolbar.tmm-toolbar a {
  color: #faecbd;
  opacity: 0.5;
}

.toolbar.tmm-toolbar:before {
  display: none;
}

.toolbar.tmm-toolbar a.active {
  opacity: 1;
}

.toolbar.tmm-toolbar i.icon {
  /*margin-top: 4px;*/
  font-size: 24px;
}

.toolbar.tmm-toolbar i.icon {
  display: none;
}

.toolbar.tmm-toolbar a [class$="-outline"] {
  display: block;
}

.toolbar.tmm-toolbar a.active [class*="ticon-"] {
  display: block;
}

.toolbar.tmm-toolbar a.active [class$="-outline"] {
  display: none;
}

.toolbar.tmm-toolbar .tabbar-label {
  font-size: 10px;
  line-height: 10px;
  /*margin-bottom: 4px;*/
}

.toolbar.tmm-toolbar.tabbar-labels a.tab-link {
  padding-top: 2px;
}


/*重要，不设置它，滚动时工具栏会重新提示*/

.views {
  overflow: hidden;
}


/*加载地图的样式*/

.tmm-more-map {
  position: fixed;
  width: 80%;
  left: 10%;
  top: 60px;
  background: #fff;
  border-radius: 4px 4px 0 0;
  z-index: 101;
}

.tmm-more-title {
  border-bottom: 1px solid #d7d9d7;
  font-size: 13px;
  padding: 10px 16px;
}

.tmm-more-map img {
  width: 100%;
}

.tmm-bg-mask {
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0.8;
  position: fixed;
  z-index: 100;
  top: 0;
  left: 0;
}

.tmm-banner-detail {
  position: fixed;
  background: transparent;
  border-radius: 5px;
  z-index: 11000;
  max-height: 80%;
  overflow: auto;
  top: 65px;
  bottom: 65px;
  margin: 0px 12px;
  height: auto;
}

.tmm-hotel-lazy {
  width: 100%;
}


/*选择随行人员  tym*/

.popup .select-retinue {
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 200;
}

.content-block.tmm-select-retinue-block {
  padding: 0 1px 0 0;
}

.tmm-select-retinue-ul {
  /*max-height: 300px;*/
  overflow: auto;
  max-height: 85%;
  width: 100%;
  position: absolute !important;
}

.list-block .tmm-select-retinue-ul .item-inner:after {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  right: auto;
  top: auto;
  height: 1.1px;
  width: 100%;
  background-color: #c8c7cc;
  display: block;
  z-index: 15;
  -webkit-transform-origin: 50% 100%;
  transform-origin: 50% 100%;
}

.tmm-select-retinue-p {
  position: absolute;
  width: 90%;
  left: 5%;
  bottom: 10px;
  z-index: 200;
}

.tmm-select-retinue-navbar {
  position: absolute;
  width: 100%;
  left: 0px;
  top: 0px;
  z-index: 200;
}


/* Moore Mo增加  --选择酒店入住和退房日期时的日历控件样式*/

.tmm-hoteldate-picker-calendar.picker-calendar {
  height: 390px;
}


/* Moore Mo增加  --选择酒店入住和退房日期时的toast样式*/

.toast-modal-bottom {
  width: 100%;
  left: 0;
  top: 95%;
  margin-left: 0;
  border-radius: 4px;
  color: #fff;
  z-index: 12000;
}

.toast-modal-bottom .modal-inner {
  display: inline-block;
  padding: 6px 15px;
  border-radius: 4px;
  background: rgba(0, 0, 0, 0.8);
}

.toast-modal-bottom .modal-title {
  font-size: 14px;
}


/* 临时样式 */

.table-group {
  margin-bottom: 20px;
}

.table-cell {
  position: relative;
  display: block;
  height: 44px;
  line-height: 44px;
  background-color: #fff;
  border-bottom: 1px solid #d7d9d7;
  font-size: 14px;
  color: #565a5c;
  -webkit-transition: all .25s;
  transition: all .25s;
}

.table-cell:active,
.table-cell.active-state {
  background-color: #d9d9d9;
}

.table-cell .ticon-arrow-right {
  position: absolute;
  right: 16px;
  color: #d7d9d7;
}

.table-cell .tit {
  margin-left: 16px;
}

.table-cell:first-of-type {
  border-top: 1px solid #d7d9d7;
}

@media (min--moz-device-pixel-ratio: 1.5),
(-webkit-min-device-pixel-ratio: 1.5),
(min-device-pixel-ratio: 1.5),
(min-resolution: 144dpi),
(min-resolution: 1.5dppx) {
  .table-cell {
    border: none;
    background-image: -webkit-linear-gradient(bottom, #d7d9d7, #d7d9d7 50%, transparent 50%);
    background-image: linear-gradient(0deg, #d7d9d7, #d7d9d7 50%, transparent 50%);
    background-position: bottom;
    background-size: 100% 1px;
    background-repeat: no-repeat;
  }
  .table-cell:first-of-type {
    border: none;
    background-image: -webkit-linear-gradient(top, #d7d9d7, #d7d9d7 50%, transparent 50%), -webkit-linear-gradient(bottom, #d7d9d7, #d7d9d7 50%, transparent 50%);
    background-image: -webkit-linear-gradient(top, #d7d9d7, #d7d9d7 50%, transparent 50%, linear-gradient(0deg, #d7d9d7, #d7d9d7 50%, transparent 50%));
    background-image: linear-gradient(180deg, #d7d9d7, #d7d9d7 50%, transparent 50%, linear-gradient(0deg, #d7d9d7, #d7d9d7 50%, transparent 50%));
    background-position: top, bottom;
    background-size: 100% 1px;
    background-repeat: no-repeat;
  }
}

