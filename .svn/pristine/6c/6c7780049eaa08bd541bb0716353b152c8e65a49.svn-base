/**************************************************************************/


/**
 * 公共css文件，公共的样式定义在这
 */


/***************************************************************************/

body {
  font-family: Helvetica, "华文细黑";
  background: #f0f1f2;
  -webkit-overflow-scrolling: touch;
}

.clearfix:before,
.clearfix:after {
  display: table;
  content: "";
}

.clearfix:after {
  clear: both;
}

ul,
ol {
  list-style: none;
}

p,
ul,
ol {
  padding: 0;
  margin: 0;
}

.gray {
  color: #313131;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.red {
  color: #ff595f;
}

.clear-before:before {
  display: none !important;
}

.version {
  display: block;
  position: absolute;
  width: 100%;
  bottom: 60px;
  z-index: 10;
  font-size: 11px;
  line-height: 11px;
  text-align: center;
  color: #999999;
}


/**
 * 下面边框
 */

.b-border {
  position: relative;
}

.b-border:before {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  right: auto;
  top: auto;
  height: 1px;
  width: 100%;
  background-color: #d7d9d7;
  display: block;
  z-index: 15;
  -webkit-transform-origin: 50% 100%;
  transform-origin: 50% 100%;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}


/**************************************************************************/


/**
 * 公共css文件，公共的样式定义在这
 */


/***************************************************************************/


/* 重写框架样式 */

html.with-statusbar-overlay body {
  /*padding-top: 0;*/
}

.statusbar-overlay {
  background: #ffffff;
  /*opacity: 0;*/
  /*height:0;*/
}

.pages {
  background: #fff;
}

.page-content {
  padding-bottom: 50px;
}

.list-block ul:after {
  background-color: #d7d9d7;
  height: 1px;
  position: static;
  -webkit-transform-origin: 0% 0%;
  transform-origin: 0% 0%;
}

.list-block ul:before {
  background-color: #d7d9d7;
}

.navbar:after {
  background-color: #d7d9d7;
}


/***************************************************************************/


/**
 * navbar.tmm-navbar
 */

.navbar.tmm-navbar,
.navbar.tmm-navbar .subnavbar {
  background-color: #FFFFFF;
  height: 40px !important;
}

.navbar.tmm-navbar:after {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  right: auto;
  top: auto;
  height: 1px;
  width: 100%;
  background-color: #000000;
  display: block;
  z-index: 15;
  opacity: 0.2;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.navbar .center {
  font-size: 20px !important;
  font-weight: normal;
}

.pull-to-refresh-layer {
  margin-top: -48px;
}

.navbar a.link,
.toolbar a.link,
.subnavbar a.link {
  line-height: 40px !important;
  height: 40px !important;
}

.navbar-through .page-content,
.navbar-fixed .page-content {
  padding-top: 40px;
}

.navbar .center {
  line-height: 40px !important;
  color: #000 !important;
}

.navbar.tmm-navbar .ion-ios-arrow-thin-left,
.navbar.tmm-navbar .ion-android-arrow-back,
.navbar.tmm-navbar .ion-ios-close-empty {
  color: #FF0000;
  font-size: 35px;
}

.navbar.tmm-navbar .navbar-inner {
  padding: 0 16px;
}

.navbar.tmm-navbar a {
  color: #000000;
  position: relative;
}

.navbar.tmm-navbar a:before {
  content: "";
  display: block;
  position: absolute;
  top: 0;
  bottom: 0;
  left: -16px;
  right: -16px;
}

.navbar.tmm-navbar .icon {
  font-size: 20px;
}

.page {
  background: #f0f2f0;
}

.tmm-my-page {
  background: #FFF;
}

.mian-navbar-line:before {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  top: 0;
  height: 1px;
  width: 100%;
  background-color: #d7d9d7;
  display: block;
  z-index: 15;
  -webkit-transform-origin: 50% 100%;
  transform-origin: 50% 100%;
  -webkit-transform: scaleY(0.5);
