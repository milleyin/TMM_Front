/* 选择支付类型窗口 */

.pay-order-choice {
  position: absolute;
  left: 20px;
  right: 20px;
  top: 150px;
  z-index: 10001;
  border-radius: 5px;
  background-color: #fff;
  translate: translate3d(0, 0, 0);
  -webkit-translate: translate3d(0, 0, 0);
  transition: all .3s;
  -webkit-transition: all .3s;
  overflow: hidden;
}

.pay-order-choice.pay-wallet {
  transform: translate3d(0, -60px, 0);
  -webkit-transform: translate3d(0, -60px, 0);
}

.pay-order-choice .pay-tit {
  position: relative;
  height: 44px;
  border-bottom: 1px solid #39ab53;
  font-size: 15px;
  line-height: 44px;
  text-align: center;
}

.pay-order-choice .pay-tit .ticon-close {
  position: absolute;
  left: 16px;
}

.pay-order-choice .pay-tit .ticon-close:after {
  content: '';
  position: absolute;
  top: 0;
  left: -16px;
  right: -16px;
  bottom: 0;
}

.pay-order-choice .pay-wrap {
  padding-left: 16px;
  padding-right: 16px;
}

.pay-order-choice .pay-wrap>div {
  border-bottom: 1px solid #d7d9d7;
}

.pay-order-choice .pay-wrap>div:last-of-type {
