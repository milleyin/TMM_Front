.tmm-login-nav {
  position: relative;
  width: 100%;
  height: 100%;
  background: #3f3936;

  .login {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 48px;
    text-align: center;
    line-height: 48px;
    background-color: #c8a063;
    z-index: 1;

    a {
      position: relative;
      display: inline-block;
      width: 49%;
      color: #111;
      font-size: 15px;
      text-decoration: none;
    }

    a.acc-login:after {
      position: absolute;
      top: 11px;
      right: 0;
      content: '';
      width: 1px;
      height: 26px;
      background: #403a39;
    }
  }
}

.tmm-acc-login-wrap {
  position: relative;
  margin-top: 60px;

  .item-input-wrapper {
    padding: 0;
    background: #fff;
  }
  .login-code {
    text-align: center;
    background-color: #ff595f;
    font-size: 14px;
    line-height: 27px;
    color: #fff;
    width: 107px;
    height: 30px;
    border: none;
  }
  .list-button {
    background: #3f3936;

    .acc-login-button {
      margin: 0;
      background-color: #ff595f;
      border: none;
      color: #fff;
    }
  }

}