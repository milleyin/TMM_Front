{{#each list_data}}
<div class="journery-item">
  <div class="con-inner select-journery-item" data-select="1" data-link="{{link}}" data-id="{{value}}" data-type="{{classliy.value}}">
    <div class="pic">
      <img src="{{image}}" alt="{{name}}">
    </div>
    <div class="describe">
      <p class="tit">{{name}}</p>
      <p class="price">￥{{price}} 起/人 {{day_num}}天行程</p>
      <p class="price">{{#if isSortForDistance}}距离：{{distance}}{{else}} 人气：{{brow}}{{/if}}</p>
    </div>
  </div>
  <!-- <label class="item-select">
    <input type="radio" name="item-radio" value="{{value}}" >
  </label> -->
  <label class="label-checkbox item-content item-select">
    <input type="radio" name="item-radio" value="{{value}}">
    <div class="item-media">
      <i class="icon icon-form-checkbox"></i>
    </div>
  </lable>
</div>
{{/each}}
