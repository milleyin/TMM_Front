/**************************************************************************/


/**
 * 推荐页面中间类容栏列表
 */


/***************************************************************************/

.tmm-list-block {
  background-color: #fff;
}

.tmm-list-block ul {
  list-style: none;
  margin: 0;
  padding: 0;
}


/* 推荐页面类容列表 */
.tmm-item-content:before {
  content: '';
  position: absolute;
  height: 1px;
  width: 100%;
  background-color: #000000;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  z-index: 15;
  opacity: 0.2;
  
}
.tmm-item-content .pic {
  position: relative;
  width: 100%;
  height: auto;
  padding-top: 66.67%;
}

.tmm-item-content .pic img {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 100%;
}

.tmm-item-content .txt {
  padding: 15px 16px 13px;
  position: relative;
  background-color: #fff;
}

.tmm-item-content .txt .title {
  font-size: 17px;
  line-height: 17px;
  color: #000000;
}
.tmm-item-content .txt .title span{
	padding-bottom:6px;
}

.tmm-item-content .txt .intro {
  font-size: 12px;
  line-height: 12px;
  color: #666666;
  margin-top: 14px;
  overflow: hidden;
  white-space: nowrap;
  width: 85%;
  text-overflow: ellipsis;
}
.tmm-item-content .txt .type.act {
  background-color: #43aff4;
}

.tmm-item-content .txt .type.line {
  background-color: #39ab53;
}

.tmm-item-content .txt .type.dot {
  background-color: #13b0cc;
}

.tmm-item-content .txt .praise {
  position: absolute;
  bottom: 16px;
  right: 16px;
  vertical-align: bottom;
}
.tmm-item-content .txt .praise .praise-distance {
  font-size: 10px;
  color: #666;
}
.tmm-item-content .txt .praise .text{
  font-size: 9px;
  line-height: 9px;
  color: #666666;
}
.tmm-item-content .txt .praise .num {
  font-size: 14px;
  line-height: 14px;
  color: #000;
}


/**
 * 点的详情页面
 */

.go-item {
  position: absolute;
  width: 100%;
  left: 0px;
  bottom: 0px;
  font-size: 18px;
  line-height: 40px;
  text-align: center;
  background-color: #39ab53;
  opacity:0.9;
  z-index: 1;
}

.go-item .tmm-button {
  color: #fff;
}

.cannt-item {
  position: absolute;
  width: 100%;
  left: 0px;
  bottom: 0px;
  font-size: 16px;
  line-height: 44px;
  text-align: center;
  background-color: rgba(75,75,75,0.9);
  z-index: 1;
}

.cannt-item .tmm-button {
  color: #fff;
}

.tmm-dot-detail {
  background-color: #fff;
}

.tmm-dot-detail .banner {
  width: 100%;
  position: relative;
}

.tmm-dot-detail .banner img {
  width: 100%;
  vertical-align: middle;
  height: auto;
}

.tmm-dot-detail .banner .mask{
  position: absolute;
  bottom: 0px;
  display: inline-block;
  width: 100%;
  background: rgba(0,0,0,0.7);
  height: 50px;
}
.tmm-dot-detail .banner a.tmm-praise-shop{
  margin-top: -5px;
  width: 33%;
  display: inline-block;
  color: #fff;
  text-align:center;
}
.tmm-dot-detail .banner .mask .ticon-praise{
  color: red;
  font-size: 32px;
}
.tmm-dot-detail .banner .mask .tmm-praise-shop span{
  font-size: 10px;
  line-height: 1px;
  margin-top: -1px;
  display: block;
  color: red;
  text-align: center;
}
.tmm-dot-detail .banner .mask .ticon-praise.gray{
  color: #fff;
  font-size: 32px;
}
.tmm-dot-detail .banner .mask .tmm-praise-shop span.gray{
  color: #fff;
}

.tmm-dot-detail .banner a.tmm-share{
  float: right;
  text-align:center;
  margin-top: -5px;
  width: 33%;
  display: inline-block;
  color: #fff;
}
.tmm-dot-detail .banner .mask .tmm-share .ticon-share{
  color: #fff;
  font-size: 32px;
}
.tmm-dot-detail .banner .mask .tmm-share span{
  font-size: 10px;
  line-height: 1px;
  margin-top: -1px;
  display: block;
  color: #fff;
  text-align: center;
}
.tmm-dot-detail .banner a.tmm-phone span{
  font-size: 10px;
  line-height: 1px;
  margin-top: 2px;
  display: block;
  color: #fff;
  text-align: center;
}

.tmm-dot-detail .banner a.tmm-phone{
  float: right;
  position: absolute;
  /*margin-top: 1px;*/
  margin-top: 3px;
  width: 33%;
  display: inline-block;
  color: #fff;
  text-align:center;
}
.tmm-dot-detail .banner a.tmm-phone img{
	position:relative;
  width:26px;
  height: 32px;
}
.tmm-dot-detail .wrap {
  padding: 0 16px;
  text-align: center;
}

.tmm-dot-detail .wrap .info {
  padding-bottom: 25px;
  padding-top:25px;
}
.wrap-item{ 
	text-align:left;
	padding:16px 15px;
}
.tmm-dot-detail .wrap h2 {
  font-size: 20px;
  line-height: 20px;
  margin: 15px 0px 12px;
  font-weight: bold;
  color:#000000;
}

.tmm-dot-detail .wrap .state {
  font-size: 14px;
  color: #666666;
  word-break: break-word;
}

.tmm-dot-detail .wrap .showmore {
  display: inline-block;
  margin: 20px 0px 25px 0px;
  font-size: 8px;
  background-color: #fff;
  color: #ff595f;
  height:8px;
  line-height:8px;
}
.tmm-dot-detail .wrap .showmore span:nth-child(2){
	padding:0px 5px;	
}

.tmm-dot-detail .wrap img {
  width: 100%;
  vertical-align: midden;
  height: 216px;
}


/**
 * 线的详情页面 */ 
.dot-go-item{
  background-color:#13b0cc;
  opacity:0.9;
 }
.bracket {
  font-size: 16px;
  -webkit-text-fill-color: white;
  -webkit-text-stroke-color: #000;
  -webkit-text-stroke-width: 0.5px;
  text-fill-color: white;
  text-stroke-color: #000;
  text-stroke-width: 0.5px;
  font-style: initial;
 }

.tmm-line-detail .line-day-tit {
  padding-top: 30px;
}

.tmm-line-detail .line-day-tit .bright {
  position: relative;
  font-size: 16px;
  padding-bottom: 10px;
  color: #616466;
  word-wrap: break-word;
}

.tmm-line-detail .line-day-tit .bright:after {
  content: "";
  border: 1px solid #39ab53;
  width: 6px;
  height: 6px;
  position: absolute;
  border-radius: 100%;
  top: 100%;
  left: 50%;
  margin-left: -4px;
  margin-top: -4px;
}

.tmm-line-detail .line-day-tit .day {
  display: inline-block;
  font-size: 11px;
  line-height: 11px;
  padding: 4px 6px;
  margin: 32px auto;
  background-color: #39ab53;
  color: #fff;
  position: relative;
  border-radius:4px;
}

.tmm-line-detail .line-day-tit .day:before {
  content: "";
  height: 28px;
  position: absolute;
  width: 1px;
  bottom: 100%;
  left: 50%;
  background-color: #39ab53;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
  -webkit-transform-origin: 0 50%;
  transform-origin: 0 50%;
}

.tmm-line-detail .line-day-tit .day:after {
  content: "";
  height: 28px;
  position: absolute;
  width: 1px;
  top: 100%;
  left: 50%;
  background-color: #61cc13;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
  -webkit-transform-origin: 0 50%;
  transform-origin: 0 50%;
}

.tmm-line-detail .info .tit {
  margin-top: 10px;
}

.tmm-line-detail .info:last-of-type {
  padding-bottom: 25px;
}

/**
 * 活动的详情页
*/
.tmm-line-detail .line-day-tit .bright.act-bright:after{
  border: 1px solid #3b9edb;
}
.tmm-line-detail .line-day-tit .day.act-day:before {
  background-color: #3b9edb;
}
.tmm-line-detail .line-day-tit .day.act-day:after{
  background-color: #3b9edb;
}
.tmm-line-detail .line-day-tit .day.act-day{
  background-color:#3b9edb;
  border-radius:4px;
}
.tmm-activity-ticon-point:before{border-top-color: #3b9edb !important;}
.act-go-item{
  background-color:#3b9edb;
}
.a-arrows-down{
  color: #b3b3b3;
}
.arrows-down{
  margin-left: 50%;
  margin-top: 20px; 
  font-style: initial;
  font-size: 14px;
}
