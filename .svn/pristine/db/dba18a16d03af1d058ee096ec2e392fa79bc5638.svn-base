/**
 * 点的订单详情
 */

.sub-num {
  display: inline-block;
  vertical-align: middle;
  height: 24px !important;
  width: 24px !important;
  position: relative;
  border: 1px solid #d7d9d7;
  border-radius: 24px;
}

.sub-num:before {
  content: '';
  height: 2px;
  width: 12px;
  display: block;
  background: #d7d9d7;
  position: absolute;
  top: 11px;
  left: 6px;
}

.add-num {
  display: inline-block;
  vertical-align: middle;
  height: 24px !important;
  width: 24px !important;
  position: relative;
  border: 1px solid #d7d9d7;
  border-radius: 24px;
}

.add-num:before,
.add-num:after {
  content: '';
  height: 2px;
  width: 12px;
  display: block;
  background: #ff595f;
  position: absolute;
  top: 11px;
  left: 6px;
}

.add-num:after {
  height: 12px;
  width: 2px;
  top: 6px;
  left: 11px;
}

