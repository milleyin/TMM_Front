<div>
  <div class="navbar tmm-navbar">
    <div class="navbar-inner">
      <div class="left">
        <a href="index.html" class="back link">
          <i class="icon ticon-arrow-back"></i>
        </a>
      </div>
      <div class="center">我的赞</div>
      <div class="right">
      </div>
    </div>
  </div>
  <div class="pages">
    <div data-page="myPraise" class="page" id="myPraise">
      <div  data-distance="50" class="page-content pull-to-refresh-content infinite-scroll">
        <!-- 默认的下拉刷新层 -->
        <div class="pull-to-refresh-layer">
          <div class="preloader"></div>
          <div class="pull-to-refresh-arrow"></div>
        </div>

        <div class="tmm-list-block">
          <ul class="tmm-myPraise-ul-list">
            {{#each myPraiseList}}
            <li class="tmm-item-content">
              <div class="pic">
                <a href="javascript:;" data-type="{{js " this.classliy.value==1 ? 'dot' : this.classliy.value==2 ? 'line' : 'act' "}}" data-link="{{link}}" class="tmm-shops-detail"><img src="{{image}}"></a>
                <div class="conColor">
                  <i class="icon ticon-praise"></i>
                </div>
              </div>
              <div class="txt">
                <div class="title">{{name}}</div>
                <div class="intro">{{info}}</div>
                <span class="type {{js " this.classliy.value==1 ? 'dot' : this.classliy.value==2 ? 'line' : 'act' "}}"> {{js " this.classliy.value==1 ? '景点' : this.classliy.value==2 ? '线路' : '活动' "}}</span>
              </div>
            </li>
            {{/each}}
          </ul>
          <div id="no-more" style="display:none"></div>
        </div>
      </div>
    </div>
  </div>
</div>
