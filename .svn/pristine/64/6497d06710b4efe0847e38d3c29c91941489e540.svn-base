.seek-content {
  margin-top: 39px;
}

.sub-nav {
  display: flex;
  margin-top: 44px;
  height: 40px;
  line-height: 38px;
  background-color: rgba(255, 255, 255, 0.9);
  white-space: nowrap;
  @extend %border-bottom;
  .subtit {
    flex: 1;
    text-align: center;
    margin: 0 2px;
    color: $darkGray;
    font-size: 13px;
    background: url(data:image/png;base64,) no-repeat center 30px;
    background-size: 6px 3px;
    &.active {
      background-image: url(data:image/png;base64,);
    }
  }
}

.seekTitle {
  position: relative;
  display: inline-block;
  height: 44px;
  .tit {
    line-height: 20px;
    font-size: 16px;
    margin-top: 5px;
  }
  .txt {
    line-height: 12px;
    font-size: 12px;
    color: $gray;
  }
  &::after {
    content: "";
    position: absolute;
    right: -20px;
    top: 14px;
    width: 16px;
    height: 16px;
    background-image: url(data:image/png;base64,);
    background-size: 16px 16px;
  }
}

.seekmask {
  position: absolute;
  top: 84px;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.6);
  padding-bottom: 49px;
  overflow-y: auto;
  .tagWrap {
    font-size: 0;
    background-color: #fff;
    padding: 8px;
    .tag {
      display: inline-block;
      width: 33.3%;

      span {
        display: block;
        margin: 8px;
        font-size: 13px;
        line-height: 30px;
        border-radius: 4px;
        border: 1px solid #d7d9d7;
        text-align: center;
        color: #858b8f;
        &.active {
          background-color: $red;
          border-color: $red;
          color: #fff;
        }
      }
    }
  }
}
