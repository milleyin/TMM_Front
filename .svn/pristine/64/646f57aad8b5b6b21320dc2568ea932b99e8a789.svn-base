<!-- 验证手机号 -->
<div class="navbar tmm-navbar">
  <div class="navbar-inner tmm-verify-phone-inner">
    <div class="left">
      <a href="#" class="back link">
        <i class="icon ticon-arrow-back"></i>
      </a>
    </div>
    <div class="center sliding">验证手机号</div>
    <div class="right">
    </div>
  </div>
</div>
<div class="pages">
  <div class="page tmm-modify-phone-page tmm-verify-phone-page" data-page="verify_phone">
  <div class="set-old-phone-wrap">
    <div class="txt">验证码已发送至手机: 133*****1103,请按提示操作。</div>
    <form class="list-block tmm-setPhone-list-Form tmm-verifyPhone-list-Form">
      <ul>
        <li class="item-content" style="display:none;">
          <div class="item-inner">
            <div class="item-title label tmm-setPhone-errlabel">错误信息</div>
          </div>
        </li>
        <li class="item-content">
          <div class="item-inner tmm-setPhone-item-inner tmm-verifyPhone-item-inner">
            <div class="item-input item-title-con">
              <input type="text" name="name" id="oldPhoneNote" placeholder="填写验证码" class="setPhoneInput">
              <span  class="get_old_code get_code">获取验证码(57)</span>
            </div>
          </div>
        </li>
      </ul>
      <div class="bg_btn mob_btn">
        <button class="sub_btn" id="oldPhoneSubmit">完成</button>
      </div>
    </form>
   </div>
  </div>

</div>
